{
  "name": "sake-finance-subgraph",
  "version": "1.0.0",
  "description": "Sake Finance V3 subgraph on Soneium (derived from Aave Protocol Subgraphs)",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/sake-finance-subgraph"
  },
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "create:local": "graph create --node http://localhost:8020 sake-finance",
